{
  "name": "vite-react-ts-swc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format:fix:src": "prettier --write './**/*.{js,jsx,cjs,mjs,css,ts,tsx,cts,mts,json,jsonc,json5,html,xml,yml}'",
    "lint:format:src": "prettier -c './**/*.{js,jsx,cjs,mjs,css,ts,tsx,cts,mts,json,jsonc,json5,html,xml,yml}'",
    "lint:format": "run-p lint:format:*",
    "lint:src": "eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json,.jsonc,.json5 --format=unix  --report-unused-disable-directives .",
    "lint": "run-p lint:*",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.37",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.13.0",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
